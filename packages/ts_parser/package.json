{
  "name": "ts_parser",
  "private": true,
  "author": "withastro",
  "license": "MIT",
  "type": "module",
  "bugs": "https://github.com/withastro/compiler/issues",
  "homepage": "https://astro.build",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "wasm:strip": "wasm-strip dist/ts_parser.wasm",
    "wasm:build": "cargo build --target wasm32-unknown-unknown --release",
    "wasm:mv": "mkdir --parents ./dist; mv ./target/wasm32-unknown-unknown/release/ts_parser.wasm dist/ts_parser.wasm",
    "build:old": "pnpm clean && pnpm wasm:build && pnpm wasm:mv && pnpm wasm:strip",
    "build": "./build.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/compiler.git"
  }
}
