{
  "name": "ts_parser",
  "author": "withastro",
  "license": "MIT",
  "type": "module",
  "bugs": "https://github.com/withastro/compiler/issues",
  "homepage": "https://astro.build",
  "version": "0.1.0",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:node": "wasm-pack build --out-dir dist/node --target nodejs",
    "build:browser": "wasm-pack build --out-dir dist/browser --target web",
    "after-build": "rm ./dist/node/package.json && rm ./dist/browser/package.json && mv ./dist/node/ts_parser.js ./dist/node/ts_parser.cjs",
    "build": "pnpm clean && pnpm build:node && pnpm build:browser && pnpm after-build"
  },
  "main": "./dist/node/ts_parser.cjs",
  "types": "./dist/node/ts_parser.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/compiler.git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/node/ts-parser.d.ts",
      "browser": "./dist/browser/ts_parser.js",
      "import": "./dist/node/ts_parser.cjs",
      "require": "./dist/node/ts_parser.cjs",
      "default": "./dist/browser/ts_parser.js"
    },
    "./browser.wasm": {
      "types": "./dist/node/ts_parser_bg.d.ts",
      "browser": "./dist/browser/ts_parser_bg.wasm",
      "import": "./dist/node/ts_parser_bg.wasm",
      "require": "./dist/node/ts_parser_bg.wasm",
      "default": "./dist/browser/ts_parser_bg.wasm"
    },
    "./node.wasm": {
      "types": "./dist/node/ts_parser_bg.d.ts",
      "browser": "./dist/browser/ts_parser_bg.wasm",
      "import": "./dist/node/ts_parser_bg.wasm",
      "require": "./dist/node/ts_parser_bg.wasm",
      "default": "./dist/browser/ts_parser_bg.wasm"
    },
    "./types": "./dist/node/ts_parser.d.ts",
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@jridgewell/trace-mapping": "^0.3.16",
    "@types/node": "^18.15.11",
    "@types/sass": "^1.43.1",
    "acorn": "^8.8.1",
    "esbuild": "^0.17.17",
    "tsup": "^6.7.0",
    "typescript": "~5.0.2"
  }
}
